#
# CAN controller: MCP2515
#
LOCATE COMP "PinCanOsc" SITE "H21" ;
IOBUF PORT "PinCanOsc" IO_TYPE=LVCMOS33 PULLMODE=UP ;
LOCATE COMP "PinCanRx_0" SITE "G21" ;
IOBUF PORT "PinCanRx_0" IO_TYPE=LVCMOS33 PULLMODE=UP ;
LOCATE COMP "PinCanRx_1" SITE "G22" ;
IOBUF PORT "PinCanRx_1" IO_TYPE=LVCMOS33 PULLMODE=UP ;
LOCATE COMP "PinCanInt" SITE "F22" ;
IOBUF PORT "PinCanInt" IO_TYPE=LVCMOS33 PULLMODE=UP ;
LOCATE COMP "PinCanCs" SITE "H20" ;
IOBUF PORT "PinCanCs" IO_TYPE=LVCMOS33 PULLMODE=UP ;
LOCATE COMP "PinCanSi" SITE "D22" ;
IOBUF PORT "PinCanSi" IO_TYPE=LVCMOS33 PULLMODE=UP ;
LOCATE COMP "PinCanSo" SITE "J20" ;
IOBUF PORT "PinCanSo" IO_TYPE=LVCMOS33 PULLMODE=UP ;
LOCATE COMP "PinCanSclk" SITE "F21" ;
IOBUF PORT "PinCanSclk" IO_TYPE=LVCMOS33 PULLMODE=UP ;
LOCATE COMP "PinCanRst" SITE "E5" ;
IOBUF PORT "PinCanRst" IO_TYPE=LVCMOS33 PULLMODE=UP ;
#
# USB/Uart bridge chip FT232:
# Note direction Tx/Rx as viewed from FPGA
#   
LOCATE COMP "PinUsbTx" SITE "D6" ;
IOBUF PORT "PinUsbTx" IO_TYPE=LVCMOS33 PULLMODE=UP ;
LOCATE COMP "PinUsbRx" SITE "A2" ;
IOBUF PORT "PinUsbRx" IO_TYPE=LVCMOS33 PULLMODE=UP ;
#
# PS2 keyboard inputs: Clk & Dat
#
LOCATE COMP "PinKbdDat" SITE "D4" ;
IOBUF PORT "PinKbdDat" IO_TYPE=LVCMOS33 PULLMODE=UP ;
LOCATE COMP "PinKbdClk" SITE "C6" ;
IOBUF PORT "PinKbdClk" IO_TYPE=LVCMOS33 PULLMODE=UP ;
