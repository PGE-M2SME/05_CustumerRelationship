#
# Interface with Video Encoder: DB15
#
# Mouse
#
LOCATE COMP "PinSclS" SITE "D18-E18" ;
IOBUF PORT "PinSclS" IO_TYPE=LVCMOS33 PULLMODE=UP ;
LOCATE COMP "PinSdaS" SITE "C9-D11" ;
IOBUF PORT "PinSdaS" IO_TYPE=LVCMOS33 PULLMODE=UP ;
